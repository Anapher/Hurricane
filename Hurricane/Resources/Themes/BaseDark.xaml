<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib"
                    xmlns:options="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="pack://application:,,,/MahApps.Metro;component/Styles/Accents/BaseDark.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <SolidColorBrush x:Key="PicturePreviewBackgroundBrush" Color="{StaticResource WhiteColor}" options:Freeze="true"/>
    <SolidColorBrush x:Key="PicturePreviewForegroundBrush" Color="White" options:Freeze="true"/>
    <SolidColorBrush x:Key="SliderBackground" Color="#FF646464" options:Freeze="true"/>
    <system:Boolean x:Key="LightPlaceHolder">True</system:Boolean>
    <system:Boolean x:Key="LightVolumeIcon">True</system:Boolean>
    <system:Boolean x:Key="UseDialogsForWhiteTheme">False</system:Boolean>
</ResourceDictionary>